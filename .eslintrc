{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "impliedStrict": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-parens": [
      "error",
      "always"
    ],
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "no-restricted-syntax": [
      "error",
      "WithStatement"
    ],
    "no-param-reassign": 1,
    "padded-blocks": 0,
    "no-plusplus": 0
  }
}
